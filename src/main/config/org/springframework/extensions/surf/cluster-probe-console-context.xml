<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>

  <!-- Add Admin Console messages -->
  <bean id="rl.cluster-probe-console.resources" class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent">
    <property name="resourceBundles">
      <list>
        <value>alfresco.messages.cluster-probe-console</value>
      </list>
    </property>
  </bean>

  <bean id="webscript.com.redpill.alfresco.clusterprobe.probe.get" class="com.redpill.alfresco.clusterprobe.share.ProbeScript" parent="webscript">
    <property name="configService" ref="web.config" />
  </bean>

</beans>